{
  "hosting": {
    "public": "dist/public",
    "rewrites": [
      {
        "source": "/healthcheck",
        "function": "healthcheck"
      },
      {
        "source": "/publish",
        "function": "publish"
      },
      {
        "source": "**/**",
        "function": "next"
      }
    ]
  },
  "functions": {
    "source": "dist/functions"
  }
}
